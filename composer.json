{
    "version": "1.0.0",
    "name": "garados007/data-layout",
    "authors": [
        {
            "name": "Max Brauer",
            "role": "owner"
        }
    ],
    "description": "experimental project to simply build data access code ",
    "require": {
        "php": "^7.3"
    },
    "require-dev": {
        "webonyx/graphql-php": "^0.13.8"
    },
    "autoload": {
        "psr-4": {
            "Build\\": "php/Build/",
            "Data\\": "php/Data/",
            "Validation\\": "php/Validation/",
            "": "php/"
        },
        "files": [
            "php/Data/Bounds.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "php/DataTest/",
            "Test\\Generated\\": "php/test/"
        },
        "files": [
            "lib/script/db.php"
        ]
    }
}